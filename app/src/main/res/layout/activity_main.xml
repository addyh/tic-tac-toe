<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.addison.tictactoe.MainActivity">


    <ImageView
        android:id="@+id/boardImageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/board"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1" />

    <ImageSwitcher
        android:id="@+id/imageSwitcher9"
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:layout_marginEnd="20dp"
        android:layout_marginLeft="21dp"
        android:layout_marginRight="20dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="24dp"
        app:layout_constraintLeft_toRightOf="@+id/imageSwitcher8"
        app:layout_constraintRight_toRightOf="@+id/boardImageView"
        app:layout_constraintTop_toBottomOf="@+id/imageSwitcher6"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <ImageSwitcher
        android:id="@+id/imageSwitcher8"
        android:layout_width="60dp"
        android:layout_height="50dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="97dp"
        app:layout_constraintBottom_toBottomOf="@+id/boardImageView"
        android:layout_marginEnd="101dp"
        app:layout_constraintRight_toRightOf="@+id/boardImageView"
        android:layout_marginTop="23dp"
        app:layout_constraintTop_toBottomOf="@+id/imageSwitcher6"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="27dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageSwitcher7"
        android:layout_marginLeft="97dp"
        android:layout_marginRight="101dp" />

    <ImageSwitcher
        android:id="@+id/imageSwitcher7"
        android:layout_width="60dp"
        android:layout_height="0dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageSwitcher8"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/boardImageView"
        app:layout_constraintTop_toTopOf="@+id/imageSwitcher8"
        android:layout_marginLeft="24dp" />

    <ImageSwitcher
        android:id="@+id/imageSwitcher6"
        android:layout_width="0dp"
        android:layout_height="50dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/imageSwitcher9"
        android:layout_marginTop="26dp"
        app:layout_constraintTop_toBottomOf="@+id/imageSwitcher3"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/imageSwitcher9" />

    <ImageSwitcher
        android:id="@+id/imageSwitcher5"
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:layout_marginEnd="93dp"
        android:layout_marginLeft="93dp"
        android:layout_marginRight="93dp"
        android:layout_marginStart="93dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="@+id/imageSwitcher4"
        app:layout_constraintRight_toRightOf="@+id/imageSwitcher6"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.49"
