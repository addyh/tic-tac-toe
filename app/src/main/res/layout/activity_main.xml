        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <ImageSwitcher
        android:id="@+id/imageSwitcher4"
        android:layout_width="0dp"
        android:layout_height="50dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
